<script>
openCity("dueToToday")
function openCity(cityName) {
    var i;
    var x = document.getElementsByClassName("bus");
    for (i = 0; i < x.length; i++) {
       x[i].style.display = "none";
    }
    $.ajax({
            method: 'get',
            url: '/api/v1/btms/travel/secured/routes',
            dataType: 'json',
            contentType: 'application/json',
            success: function (response) {

                    let data = JSON.parse(JSON.stringify(response));
                    var list = distinct_destination(data.travel_routes);

                    options = list;
                    $('#destination_option_select').empty();
                    $.each(options, function(i, p) {
                        $('#destination_option_select').append($('<option></option>').val(p).html(p));
                    });
            }
    })

    document.getElementById(cityName).style.display = "block";

}
</script>
