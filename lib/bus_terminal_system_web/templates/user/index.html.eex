<!--########################### NAPSA SEARCH ###########################-->
<div class="modal fade" id="exampleModalLong" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">
            <b>Social Security Number Search</b>
        </h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="panel-body">
            <div class="row">
                <div class="col-md-12">
                    <fieldset>
                        <legend class="text-semibold"><i class="icon-reading position-left"></i> NAPSA SEARCH</legend>

                        <div class="form-group">
                            <label>SSN:</label>
                            <input name="ssn" class="form-control" placeholder="Social Security Number" type="text">
                            <input type="hidden" name="_csrf_token" value="<%= Plug.CSRFProtection.get_csrf_token()%>">
                        </div>

                        <div style="margin-bottom: 15px">
                            OR SEARCH WITH
                        </div>

                        <div class="form-group">
                            <label>NRC:</label>
                            <input name="uuid" class="form-control" placeholder="National ID" type="text">
                        </div>
                    </fieldset>
                </div>
            </div>

            <div class="text-right">
            <%= link "Search", to: Routes.user_path(@conn, :new), style: "border-radius: 12px", class: "w3-btn w3-indigo w3-hover-orange" %>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>
