<!-------------------------- Edit Modal ------------------------------------------------------------------------------------------------------------>
<div id="edit" class="modal fade" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header w3-indigo">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Marketeer Stand Allocation</h4>
      </div>
     
      <div class="modal-body">
        	<form class="form-vertical" action="#" method="post">
		        <div class="row">
		            <div class="col-md-12">
		                <div class="row">
		                    <div class="col-md-12">
		                        <div class="form-group">
		                           <!-- <label>Stand Section:</label> -->
		                          <!--  <input id="stand_section" name="" class="form-control" placeholder="Stand Section" type="text" required="required"> -->
		                            <input type="hidden" name="_csrf_token" value="<%= Plug.CSRFProtection.get_csrf_token()%>">
		                            <input type="hidden" name="" value="<%= BusTerminalSystem.Randomizer.randomizer(16,:all) %>">
		                        </div>

                                  <div class="form-group">
                                    <label id="model_name_username">Name: </label>
                                </div>

                                <div class="form-group">
                                    <label>Choose Market:</label>
                                    <select id="market_model_list" onClick="pull_section_list_by_market_id()" class="w3-select w3-border" name="market_model_id">
                                      <option value="" disabled selected>Choose Market:</option>
                                    </select>
                                </div>

                                <div class="form-group" hidden id="market_section_div" >
                                    <label>Choose Section:</label>
                                    <select id="market_section_model_list" onclick="pull_stand_list_by_section_id()" class="w3-select w3-border" name="market_section_model_id">
                                      <option value="" disabled selected>Choose Section:</option>
                                    </select>
                                </div>

                                <div class="form-group" id="market_stand_div" >
                                    <label>Choose Stand:</label>
                                    <select id="market_stand_model_list" class="w3-select w3-border" name="market_shop_model_id">
                                      <option value="" disabled selected>Choose Shop:</option>
                                    </select>
                                </div>

                                <div class="form-group" id="market_stand_div" >
                                    <label>Stand Price:</label>
                                    <input id="stand_price" name="stand_price" class="form-control" placeholder="Stand Price" type="number" required="required">
                                </div>

		                        <div hidden class="form-group">
		                            <label>Stand Name:</label>
		                            <input id="stand_name" name="" class="form-control" placeholder="Stand Name" type="text" required="required">
		                        </div>

		                    </div>
		                </div>     
		            </div>
		        </div>
		        <br>
		        <div class="text-right" id="stand_allocate_button">
		            <button type="submit" onClick="allocate_stand()"  class="w3-btn w3-indigo w3-hover-orange w3-left w3-round">
		                <b><i class="icon-checkmark2"></i></b>Allocate
		            </button>                  
		        </div>
		                    
		    </form>
      </div>
      <hr>
		      	<div class="modal-footer">
		        	<button type="button" class="w3-btn w3-grey w3-round" data-dismiss="modal">Close</button>
		      	</div>
    </div>
  </div>
</div>
